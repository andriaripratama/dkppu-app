@model DKUPPU.Models.t_mst_applicant

@{
    ViewBag.Title = "Delete";
}

<div class="jarviswidget jarviswidget-sortable" data-widget-editbutton="false" data-widget-colorbutton="false" role="widget">
    <header>
        <span class="widget-icon"><i class="fa fa-plus"></i></span>
        <h2>Detail Applicant </h2>
        <span class="jarviswidget-loader">
            <i class="fa fa-refresh fa-spin"></i>
        </span>
    </header>
    <div role="content">
        <div class="jarviswidget-editbox"> </div>
        <div class="widget-body">
            @using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "form-horizontal"}))
            {
                @Html.AntiForgeryToken()
                @Html.Partial("_Details", Model)
                <div class="form-actions">
                    <a href="@Url.Action("Index","Applicant")" class="btn btn-success"> Back To List </a>
                    <button href="@Url.Action("Edit","Applicant",new {id=Model.id_applicant})" class="btn btn-danger" type="submit">Delete</button>
                </div>
            }
        </div>
    </div>
</div>