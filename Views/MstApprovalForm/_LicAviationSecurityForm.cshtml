@model IEnumerable<DKUPPU.ViewModels.TrxLicAviationSecurityViewModel>
@{
    Layout = null;
}
<div class="table-responsive">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Date Of Recurrent</th>
                <th>Valid Until</th>
            </tr>
        </thead>
        <tbody id="TrxLicAviationSecurityViewModel">
            @{int j = 0;}
            @foreach (var item in Model)
            {
                <tr id="TrxLicAviationSecurityViewModel_@j">
                    <td>
                        <input type="hidden" name="TrxLicAviationSecurityViewModel[@j].id_trx_aviation_security" id="TrxLicAviationSecurityViewModel_@j.ToString()__id_trx_aviation_security" value="@Html.DisplayFor(a => item.id_trx_aviation_security)">
                        <input type="text" class="form-control date-picker" name="TrxLicAviationSecurityViewModel[@j].recurrent_date" id="TrxLicAviationSecurityViewModel_@j.ToString()__recurrent_date" value="@Html.DisplayFor(a => item.recurrent_date)" readonly="readonly">
                    </td>
                    <td><input type="text" class="form-control date-picker" name="TrxLicAviationSecurityViewModel[@j].expired_date" id="TrxLicAviationSecurityViewModel_@j.ToString()__expired_date" value="@Html.DisplayFor(a => item.expired_date)" readonly="readonly"></td>
                </tr>
                j++;
            }
        </tbody>
    </table>
</div>




