@model IEnumerable<DKUPPU.ViewModels.TrxLicCrewResourceManagementViewModel>
@{
    Layout = null;
}
<div class="table-responsive">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Date Of Recurrent</th>
                <th>Valid Until</th>
            </tr>
        </thead>
        <tbody id="TrxLicCrewResourceManagementViewModel">
            @{int j = 0;}
            @foreach (var item in Model)
            {
                <tr id="TrxLicCrewResourceManagementViewModel_@j">
                    <td>
                        <input type="hidden" name="TrxLicCrewResourceManagementViewModel[@j].id_trx_crew_rsc_mgt" id="TrxLicCrewResourceManagementViewModel_@j.ToString()__id_trx_crew_rsc_mgt" value="@Html.DisplayFor(a => item.id_trx_crew_rsc_mgt)">
                        <input type="text" class="form-control" name="TrxLicCrewResourceManagementViewModel[@j].recurrent_date" id="TrxLicCrewResourceManagementViewModel_@j.ToString()__recurrent_date" value="@Html.DisplayFor(a => item.recurrent_date)" readonly>
                    </td>
                    <td><input type="text" class="form-control" name="TrxLicCrewResourceManagementViewModel[@j].expired_date" id="TrxLicCrewResourceManagementViewModel_@j.ToString()__expired_date" value="@Html.DisplayFor(a => item.expired_date)" readonly></td>
                </tr>
                j++;
            }
        </tbody>
    </table>
</div>




