@model DKUPPU.ViewModels.TrxLicToeflCertificateViewModel
@{
    Layout = null;
}
<div class="row">
    <div class="col-xs-12">
        <div class="form-horizontal">
            <div class="form-group">
                <label class="col-md-2 control-label text-left" for="sertificate_no">Certificate Number *</label>
                <div class="col-md-5">
                    <input name="TrxLicToeflCertificateViewModel.toefl_certi_no" id="TrxLicToeflCertificateViewModel_toefl_certi_no" class="form-control required-value" placeholder="Input Certificate Number" />
                    @Html.ValidationMessage("TrxLicToeflCertificateViewModel.toefl_certi_no", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-2 control-label text-left" for="sertificate_no">TOEIC Score *</label>
                <div class="col-md-5">
                    <input name="TrxLicToeflCertificateViewModel.toefl_test_score" id="TrxLicToeflCertificateViewModel_toefl_test_score" class="form-control required-value" placeholder="Input Toeic Score" maxlength="3"  />
                </div>
            </div>
        </div>
    </div>
    
</div>