@model DKUPPU.ViewModels.TrxLicenseUploadViewModel
@{
    Layout = null;
}
<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-3">
            <input type="file" class="btn btn-default" name="TrxLicenseUploadViewModel.File" id="LicenseScan_File" />
            <input type="hidden" name="TrxLicenseUploadViewModel.id_trx_license_upload" id="LicenseScan_id_doc_category" value="@Html.DisplayFor(item=>item.id_trx_license_upload)" required />
        </div>
        <div class="col-md-5">
            <input name="TrxLicenseUploadViewModel.id_trx_license" id="TrxLicenseUploadViewModel_id_trx_license" class="form-control" value="" readonly="readonly" />
        </div>
    </div>
</div>
<script>
    $(function () {
        $('#upload-scan').click(function () {
            $('#LicenseScan_id_doc_category').click();
        });
    });
</script>