@model DKUPPU.ViewModels.TrxLicMedicalCertificateViewModel
@{
    Layout = null;
}
<div class="row">
    <div class="col-xs-12">
        <div class="form-horizontal">
            <div class="form-group">
                <label class="col-md-2 control-label text-left" for="sertificate_no">Medical Class</label>
                <div class="col-md-5">
                    <input name="TrxLicMedicalCertificateViewModel.med_certi_no" id="TrxLicMedicalCertificateViewModel_med_certi_no" class="form-control required-value" value="@Html.DisplayFor(a => a.med_certi_no)" readonly="readonly" type="hidden"/>
                    <input name="TrxLicMedicalCertificateViewModel.med_certi_name" id="TrxLicMedicalCertificateViewModel_med_certi_name" class="form-control" placeholder="Input" value="@Html.DisplayFor(m => m.med_certi_name)" readonly="readonly" />
                    <input type="hidden" name="TrxLicMedicalCertificateViewModel.id_trx_lic_med_certi" id="TrxLicMedicalCertificateViewModel_id_trx_lic_med_certi" value="@Html.DisplayFor(a => a.id_trx_lic_med_certi)" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-2 control-label text-left" for="sertificate_no">The period of validity</label>
                <div class="col-md-5">
                    <div class="col-xs-6 no-padding">
                        <input name="TrxLicMedicalCertificateViewModel.from_date" id="TrxLicMedicalCertificateViewModel_from_date" class="form-control required-value date-picker" value="@Html.DisplayFor(a => a.from_date)" />
                    </div>
                    <div class="col-xs-6 no-padding">
                        <input name="TrxLicMedicalCertificateViewModel.until_date" id="TrxLicMedicalCertificateViewModel_until_date" class="form-control required-value date-picker" value="@Html.DisplayFor(a => a.until_date)" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</div>